<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted" >
            <Key latin:keyLabel="" />
            <!--X ँ DEVANAGARI SIGN CANDRABINDU-->
            <Key
                latin:keyLabel="&#x0901;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--C ण DEVANAGARI LETTER NNA-->
            <Key
                latin:keyLabel="&#x0923;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <Key latin:keyLabel="" />
            <Key latin:keyLabel="" />
            <!--N ळ DEVANAGARI LETTER LLA-->
            <Key
                latin:keyLabel="&#x0933;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--M श DEVANAGARI LETTER SHA-->
            <Key
                latin:keyLabel="&#x0936;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <Key
                latin:keyLabel="&lt;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--\> । DEVANAGARI DANDA-->
            <Key
                latin:keyLabel="&#x0964;"
                latin:moreKeys="&gt;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--\? य़ DEVANAGARI LETTER YYA-->
            <Key
                latin:keyLabel="\?"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
        </case>

        <default>
            <Key latin:keyLabel="" />
            <!--x ं DEVANAGARI SIGN ANUSVARA-->
            <Key
                latin:keyLabel="&#x0902;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--c म DEVANAGARI LETTER MA-->
            <Key
                latin:keyLabel="&#x092E;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--v न DEVANAGARI LETTER NA-->
            <Key
                latin:keyLabel="&#x0928;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--b व DEVANAGARI LETTER VA-->
            <Key
                latin:keyLabel="&#x0935;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--n ल DEVANAGARI LETTER LA-->
            <Key
                latin:keyLabel="&#x0932;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--m स DEVANAGARI LETTER SA-->
            <Key
                latin:keyLabel="&#x0938;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <Key
                latin:keyLabel=","
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <Key
                latin:keyLabel="."
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
            <!--/ य DEVANAGARI LETTER YA-->
            <Key
                latin:keyLabel="&#x092F;"
                latin:moreKeys="/"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
         </default>
    </switch>
</merge>
