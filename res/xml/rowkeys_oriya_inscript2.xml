<merge
    xmlns:latin="http://schemas.android.com/apk/res-auto"
>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted"
        >
            <!--Q ଔ ORIYA LETTER AU-->
            <Key
                latin:keyLabel="&#x0B14;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--W ଐ ORIYA LETTER AI-->
            <Key
                latin:keyLabel="&#x0B10;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--E ଆ ORIYA LETTER AA-->
            <Key
                latin:keyLabel="&#x0B06;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--R ଈ ORIYA LETTER II-->
            <Key
                latin:keyLabel="&#x0B08;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--T ଊ ORIYA LETTER UU-->
            <Key
                latin:keyLabel="&#x0B0A;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--Y ଭ ORIYA LETTER BHA-->
            <Key
                latin:keyLabel="&#x0B2D;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--U ଙ ORIYA LETTER NGA-->
            <Key
                latin:keyLabel="&#x0B19;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--I ଘ ORIYA LETTER GHA-->
            <Key
                latin:keyLabel="&#x0B18;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--O ଧ ORIYA LETTER DHA-->
            <Key
                latin:keyLabel="&#x0B27;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--P ଝ ORIYA LETTER JHA-->
            <Key
                latin:keyLabel="&#x0B1D;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--\{ ଢ ORIYA LETTER DDHA-->
            <Key
                latin:keyLabel="&#x0B22;"
                latin:moreKeys="{"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--\} ଞ ORIYA LETTER NYA-->
            <Key
                latin:keyLabel="&#x0B1E;"
                latin:moreKeys="}"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <Key
                latin:keyLabel="|"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
        </case>

        <default>
            <!--q ୌ ORIYA VOWEL SIGN AU-->
            <Key
                latin:keyLabel="&#x0B4C;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--w ୈ ORIYA VOWEL SIGN AI-->
            <Key
                latin:keyLabel="&#x0B48;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--e ା ORIYA VOWEL SIGN AA-->
            <Key
                latin:keyLabel="&#x0B3E;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--r ୀ ORIYA VOWEL SIGN II-->
            <Key
                latin:keyLabel="&#x0B40;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--t ୂ ORIYA VOWEL SIGN UU-->
            <Key
                latin:keyLabel="&#x0B42;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--y ବ ORIYA LETTER BA-->
            <Key
                latin:keyLabel="&#x0B2C;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--u ହ ORIYA LETTER HA-->
            <Key
                latin:keyLabel="&#x0B39;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--i ଗ ORIYA LETTER GA-->
            <Key
                latin:keyLabel="&#x0B17;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--o ଦ ORIYA LETTER DA-->
            <Key
                latin:keyLabel="&#x0B26;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--p ଜ ORIYA LETTER JA-->
            <Key
                latin:keyLabel="&#x0B1C;"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--\[ ଡ ORIYA LETTER DDA-->
            <Key
                latin:keyLabel="&#x0B21;"
                latin:moreKeys="["
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <!--\] ଼ ORIYA SIGN NUKTA-->
            <Key
                latin:keyLabel="&#x0B3C;"
                latin:moreKeys="]"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />

            <Key
                latin:keyLabel="\\"
                latin:keyLabelFlags="fontNormal|followKeyLargeLabelRatio" />
         </default>
    </switch>
</merge>
